{
    "manifest_version": 3,
    "name": "Secure Proxy Extension",
    "description": "A secure proxy extension with region selection and state persistence.",
    "version": "1.2",
    "permissions": [
      "proxy",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }